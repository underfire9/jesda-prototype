<% I18n.locale = "zh-TW" %>
<!doctype html>
<html lang="<%= I18n.locale %>" class="no-js">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
<title>{徵案主題名稱}</title>
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<% if content_for? :replace_css %>
    <%= yield :replace_css %>
<% else %>
    <link rel="stylesheet" media="screen, projection, print" href="../stylesheets/bootstrap.css">
    <link rel="stylesheet" media="screen, projection, print" href="../stylesheets/enlist.css">
    <link rel="icon" href="../assets/favicon.png" type="image/png">
<% end %>
<% if content_for? :page_styles %>
    <%= yield :page_styles %>
<% end %>
<script src="../javascripts/modernizr.custom.js"></script>
<% if content_for? :head_js %>
    <%= yield :head_js %>
<% end %>
</head>
<body>
    <div class="workspace" canvas="container">
        <%= yield %>
    </div>
    
    <script src="../javascripts/jquery-3.4.1.min.js"></script>
    <script src="../javascripts/popper.min.js"></script>
    <script src="../javascripts/bootstrap.bundle.js"></script>

    <!--%= livereload_js if ENV["RACK_ENV"] != "production" && defined?(App)%-->
    <%= yield :js %>
</body>
</html>
