                        <div>
                            <div id="map">
                                <svg id="svg"></svg>
                            </div>
                        </div>
                    </div>
                    <div class="shop-list">
                        <h1>{{ h1 }}</h1>
                        <h2>{{ h2 }}</h2>
                    </div>
                </div>
            </div>

            <div class="mobile-maparea">
                <hgroup>
                    <h3>選擇區域</h3>
                    <h4>Area Select</h4>
                </hgroup>
                <ul class="menu-item area-list">
                    <li><a href="#" title="{連結名稱}">臺北市<span class="en">Taipei City</span></a></li>
                    <li><a href="#" title="{連結名稱}">新北市<span class="en">New Taipei City</span></a></li>
